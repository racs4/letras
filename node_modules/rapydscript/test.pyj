class Rectangle:
    def __init__(self, w=1, h=1):
        self.width = w
        self.height = h
        
    def getArea(self):
        return self.width*self.height

    @staticmethod
    def isRectangle(object):
        return isinstance(object, this)
    
a = Rectangle(5, 10)
a.getArea() == Rectangle.getArea(a)
print(Rectangle.isRectangle(a))

a = f"boo ${a} c"
a = r"bee ${a} c"
